<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.*?>
<DialogPane prefHeight="265.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/18"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.chat.controllers.RightClickUsersController" fx:id="dialog_pane">
    <content>
        <Pane prefHeight="52.0" prefWidth="200.0">
            <Button layoutX="25.0" layoutY="79.0" mnemonicParsing="false" prefHeight="34.0" prefWidth="150.0"
                    text="Upgrade" fx:id="btn_upgrade">
                <font>
                    <Font size="14.0"/>
                </font>
            </Button>
            <Text layoutX="59.0" layoutY="47.0" strokeType="OUTSIDE" strokeWidth="0.0" text="User : Ali">
                <font>
                    <Font name="System Bold" size="20.0"/>
                </font>
            </Text>
            <Button layoutX="25.0" layoutY="127.0" mnemonicParsing="false" prefHeight="34.0" prefWidth="150.0"
                    text="Remove" onAction="#btn_removeUser" fx:id="btn_remove">
                <font>
                    <Font size="14.0"/>
                </font>
            </Button>
            <Button layoutX="25.0" layoutY="172.0" mnemonicParsing="false" prefHeight="34.0" prefWidth="150.0"
                    text="Send Massage">
                <font>
                    <Font size="14.0"/>
                </font>
            </Button>
            <Button layoutX="25.0" layoutY="216.0" mnemonicParsing="false" prefHeight="34.0" prefWidth="150.0"
                    text="Back" onAction="#btn_back">
                <font>
                    <Font size="14.0"/>
                </font>
            </Button>
        </Pane>
    </content>
</DialogPane>
